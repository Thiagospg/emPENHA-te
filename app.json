    "userInterfaceStyle": "light",
    "notification": {
       "icon": "./assets/icon_notification.png",
       "color": "#622565"
    },
    "plugins": [
       [
          "expo-notifications",
          {
            "icon": "./assets/icon_notification.png",
            "color": "#622565",
